{
  "name": "docker-decoder-stream",
  "version": "1.0.0",
  "description": "JS/TS decoder for Docker logs stream",
  "author": "Viacheslav Azarov",
  "license": "MIT",
  "scripts": {
    "build": "tsup --dts",
    "test": "jest"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "es2015": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "eventemitter3": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "Docker",
    "Stream",
    "WebStream",
    "Logs",
    "Decoder"
  ]
}